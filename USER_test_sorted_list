#Test normal


def test_normal():
    test_arr = ArraySortedList(6)

    a = ListItem('one', 1)
    b = ListItem('two', 2)
    c = ListItem('three', 2)
    d = ListItem('four', 2)
    e = ListItem('five', 4) #test stable
    f = ListItem('six', 4)

    list_items = [a,b,c,d,e,f]

    for item in list_items:
        # print("This item: ",item)
        test_arr.add_stable(item)


    # for _ in test_arr:
    #     print(_)
    return test_arr

